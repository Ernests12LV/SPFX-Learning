    "group": { "default": "Advanced" },
    "title": { "default": "tempalte-1-20" },
    "description": { "default": "tempalte-1-20 description" },
    "officeFabricIconFontName": "PinSolid12",
    "properties": {
      "description": "tempalte-1-20",
      "productName": "Sample Product from Manifest",
      "productDescription": "This is a sample product description",
      "productCost": 100,
      "quantity": 1,
      "isCertified": false,
      "rating": 5,
      "category": "electronics",
      "deliveryOption": "standard",
      "features": [],
      "paymentMethod": "credit",
      "colorScheme": "light"
    }
  }]
}

